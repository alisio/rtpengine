[rtpengine]
table = -1
# no-fallback = false
### for userspace forwarding only:
# table = -1

### a single interface:
interface = <%= @rtpengine_listen_interface %>
### separate multiple interfaces with semicolons:
# interface = internal/12.23.34.45;external/23.34.45.54
### for different advertised address:
# interface = 12.23.34.45!23.34.45.56

listen-ng = <%= @rtpengine_ctrl_socket %>
# listen-tcp = 25060
# listen-udp = 12222

timeout = 60
silent-timeout = 3600
tos = 184
#control-tos = 184
# delete-delay = 30
# final-timeout = 10800

foreground = false
# pidfile = /run/ngcp-rtpengine-daemon.pid
# num-threads = 16

port-min = <%= @rtpengine_min_port %>
port-max = <%= @rtpengine_max_port %>
max-sessions = <%= @rtpengine_max_sessions %>

# recording-dir = /var/spool/rtpengine
